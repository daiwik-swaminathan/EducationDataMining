# E203: Whitespace before ':' - ignored for compatibility with Black
# W503: Line break before binary operator - ignored for compatibility with Black
[flake8]
max-line-length = 9999
exclude = 
    .git,
    __pycache__,
    .venv,
    env,
    build,
    dist
extend-ignore = 
    E203,
    W503,
    E402
select = 
    C,
    E,
    F,
    W,
    B,
    D 
